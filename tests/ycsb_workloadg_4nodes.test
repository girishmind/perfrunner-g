[test_case]
test = perfrunner.tests.ycsb.YCSBThroughputTest

[showfast]
title = Avg Throughput (ops/sec), Workload G, 4 nodes
component = kv
category = ycsb

[cluster]
mem_quota = 102400
initial_nodes = 4
num_buckets = 1
kernel_mem_limit = 128G
kernel_mem_limit_services = kv

[bucket]
replica_number = 1
eviction_policy = fullEviction

[load]
workload_path = workloads/workloadg
items = 100000000
workers = 160
field_length = 6000
field_count = 1

[access]
workload_path = workloads/workloadg
items = 100000000
ops = 1000000000
time = 1800
boost = 12
workers = 160
workload_instances = 6
