[test_case]
test = perfdaily.ycsb.YCSBN1QLThroughputTest

[showfast]
title = Workload E, 10M items, MOI, single node
component = YCSB

[stats]
server_processes = projector

[cluster]
mem_quota = 20480
index_mem_quota = 100000
initial_nodes = 1
num_buckets = 1
ipv6 = 1

[compaction]
db_percentage = 100

[bucket]
replica_number = 0

[secondary]
indexer.settings.storage_mode = memory_optimized

[load]
workload_path = workloads/workloade
items = 10000000
workers = 120

[index]
statements =
    CREATE PRIMARY INDEX primary_idx ON `bucket-1`;

[access]
workload_path = workloads/workloade
items = 10000000
workers = 160
ops = 10000000
time = 600
